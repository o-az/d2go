{
  "name": "d2go",
  "description": "Simply a developer console in your mobile and PC, Safari and Firefox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt --browser firefox",
    "build": "wxt build",
    "build:firefox": "wxt build --browser firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip --browser firefox",
    "check": "biome check --write . --unsafe",
    "compile": "tsc --noEmit --project tsconfig.json",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@wxt-dev/storage": "^1.2.5",
    "eruda": "^3.4.3",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@types/bun": "^1.3.0",
    "@types/node": "^24.7.2",
    "@types/webextension-polyfill": "^0.12.4",
    "@wxt-dev/auto-icons": "^1.1.0",
    "typed-query-selector": "^2.12.0",
    "typescript": "^5.9.3",
    "wxt": "^0.20.11"
  },
  "overrides": {
    "wxt": "0.20.11",
    "@wxt-dev/browser": "0.1.4",
    "@wxt-dev/storage": "1.2.5"
  },
  "license": "MIT",
  "repository": "github:o-az/d2go",
  "author": "Omar Aziz <omaraziz.dev@gmail.com>"
}
