{
  "name": "d2go",
  "description": "Handy dev console in your mobile and PC, Chromium, Safari & Firefox",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt --browser firefox",
    "build": "wxt build",
    "build:firefox": "wxt build --browser firefox",
    "build:safari": "wxt build --browser safari",
    "build:all": "bun run build && bun run build:firefox && bun run build:safari",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip --browser firefox",
    "convert-safari": "xcrun safari-web-extension-converter .output/safari-mv2",
    "check": "biome check --write . --unsafe",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "eruda": "^3.4.3",
    "@wxt-dev/storage": "^1.2.5",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.0",
    "@types/node": "^24.9.1",
    "@biomejs/biome": "^2.2.7",
    "@wxt-dev/auto-icons": "^1.1.0",
    "typed-query-selector": "^2.12.0",
    "typescript": "^5.9.3",
    "wxt": "^0.20.11"
  },
  "overrides": {
    "wxt": "0.20.11",
    "@wxt-dev/browser": "0.1.4",
    "@wxt-dev/storage": "1.2.5"
  },
  "packageManager": "bun@1.3.1",
  "license": "MIT",
  "repository": "github:o-az/d2go",
  "author": "Omar Aziz <omaraziz.dev@gmail.com>"
}
